<h1>Display your Pirate here</h1>
    <% @pirate.each do |pirate| %>
        <%= pirate.name %> 
        <%= pirate.weight %> 
        <%= pirate.height %>
    <% end %>
<h2>Display your first ship here</h2>
    <% @pirate.ships.each do |ship| %>
        <li> Name: <%= ship.first.name %></li>
        <li> Category: <%= ship.first.category %></li>
        <li> Booty: <%= ship.first.booty %></li>
    <% end %>
<h2>Display your second ship here</h2>
    <% @pirate.ships.each do |ship| %>
        <li> Name: <%= ship.last.name %></li>
        <li> Category: <%= ship.last.category %></li>
        <li> Booty: <%= ship.last.booty %></li>
    <% end %